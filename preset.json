{
  "useConfigFiles": true,
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-router": {
      "historyMode": false
    },
    "@vue/cli-plugin-vuex": {},
    "@vue/cli-plugin-eslint": {
      "config": "base",
      "lintOn": [
        "save"
      ]
    },
    "vue-cli-plugin-vuetify": {
      "preset": "configure",
      "replaceComponents": true,
      "useTheme": false,
      "useCustomProperties": true,
      "iconFont": "mdi",
      "installFonts": true,
      "useAlaCarte": true,
      "usePolyfill": true,
      "locale": "en",
      "version": "^2.4.3",
      "install": "default"
    }
  },
  "cssPreprocessor": "dart-sass"
}
